{
  "manifest_version": 2,
  "name": "page-recorder-dev",
  "version": "0.1.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost:3001; object-src 'self';",
  "permissions": [
    "tabCapture"
  ]
}